<div{{ attributes.addClass('pt-5 listServices') }}>
	<div class="container">
		  {{ title_prefix }}
		  {% if label %}
		  <div class="w-100 text-center mb-5">
			<h2{{ title_attributes.addClass('text-white') }}>{{ label }}</h2>
		  </div>
		  {% endif %}
		  {{ title_suffix }}
		  {% block content %}
		  		<div class="row"> 
				{{ content }}
				</div>
		  {% endblock %}
		  <div class="w-100 text-center mb-5">
			<a href="/services" type="button" class="btn-lg btn-warning font-weight-bold mt-4">Все услуги</a>
		  </div>
	</div>  
</div>